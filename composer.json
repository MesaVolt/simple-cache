{
    "name": "mesavolt/simple-cache",
    "description": "Simple cache service.",
    "keywords": ["simple", "cache", "simple-cache"],
    "license": "MIT",
    "authors": [
        {
            "name": "MesaVolt",
            "email": "contact@mesavolt.com"
        }
    ],
    "type": "library",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "symfony/cache": "^3.0||^4.0"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Mesavolt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mesavolt\\Tests\\": "tests/"
        }
    }
}
